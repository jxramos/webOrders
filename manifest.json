    {
      "matches": [
        "https://www.walmart.com/orders*"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/walmart_invoice.js"
      ]
    },
    {
      "matches": [
        "https://www.walmart.com/receipt-details?sbt=1"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/walmart_receipt_lookup_invoice.js"
      ]
