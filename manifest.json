      "js": [
        "common/common.js",
        "invoice_scripts/geico_invoice.js"
      ]
    },
    {
      "matches": [
        "https://GoSun.com/checkouts/cn/*/en-us/thank-you*"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/gosun_invoice.js"
      ]
    },
    {
      "matches": [
        "https://grip6.com/checkouts/cn/*/en-us/thank-you?_r=*"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/grip6_invoice.js"
      ]
    },
    {
      "matches": [
        "https://www.homedepot.com/order/view/ereceipt/summary"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/home_depot_invoice.js"
      ]
