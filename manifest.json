      ],
      "js": [
        "common/common.js",
        "invoice_scripts/traps_direct_invoice.js"
      ]
    },
    {
      "matches": [
        "https://www.vitacost.com/MyAccount/OrderSummary.aspx*"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/vitacost_invoice.js"
      ]
    },
    {
      "matches": [
        "https://www.walgreens.com/ordercapture/order-ui?checkout=*"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/walgreens_invoice.js"
      ]
    },
    {
      "matches": [
        "https://www.walmart.com/account/wmpurchasehistory*"
      ],
      "js": [
