{
  "manifest_version": 3,
  "name": "Web Orders",
  "description": "Extension to save online order Invoices to structured file on disk.",
  "version": "0.1",
  "action": {},
  "background": {
    "service_worker": "common/service-worker.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.amazon.com/gp/legacy/css/summary/print.html*"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/amazon_legacy_invoice.js"
      ]
    },
    {
      "matches": [
        "https://*.amazon.com/spr/returns*"
      ],
      "js": [
        "invoice_scripts/amazon_return.js"
      ]
    },
    {
      "matches": [
        "https://*.amazon.com/gp/digital/your-account/order-summary.html*"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/amazon_digital_invoice.js"
      ]
    },
    {
      "matches": [
        "https://primenow.amazon.com/orderStatus?orderId=*"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/primenow_invoice.js"
      ]
    },
    {
      "matches": [
        "https://*.amazon.com/gp/your-account/order-details*"
      ],
      "js": [
        "common/common.js",
        "cleanup_scripts/amazon_order_details.js"
      ]
    },
    {
      "matches": [
        "https://www.appliancepartspros.com/checkout-confirm.aspx"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/appliance_parts_pros_invoice.js"
      ]
    },
    {
      "matches": [
        "https://*.appfolio.com/connect/payments_flow_v2/payments/confirmation?mode=oneTimePayment"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/appfolio_invoice.js"
      ]
    },
    {
      "matches": [
        "https://insured.bambooinsurance.com/login/secureCheckout?pol_num=*"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/bamboo_insurance_payment.js"
      ]
    },
    {
      "matches": [
        "https://www.bayareafastrak.org/vector/invoices/invoiceViewAllDetail.do"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/fastrak_invoice.js"
      ]
    },
    {
      "matches": [
        "https://shop.blueland.com/checkouts/cn/*/en-us/thank-you?_r=*"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/blueland_invoice.js"
      ]
    },
    {
      "matches": [
        "https://secure.chase.com/web/auth/dashboard#/dashboard/*flyout=payCardConfirm*"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/chase_payment.js"
      ]
    },
    {
      "matches": [
        "https://online.citi.com/US/*/payments/crecarpay/success"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/citi_payment.js"
      ]
    },
    {
      "matches": [
        "https://www.costco.com/myaccount/*"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/costco_invoice.js"
      ]
    },
    {
      "matches": [
        "https://www.costcochecks.com/ConfirmationRs.aspx*"
      ],
      "js": [
        "common/common.js",
        "invoice_scripts/costco_checks_invoice.js"
      ]
